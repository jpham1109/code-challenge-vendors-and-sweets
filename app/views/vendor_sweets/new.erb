
<h1> New VendorSweet! </h1>
<main>
  <% if @errors %>
    <ul>
    <% @errors.each do |error| %>
      <li><%= error %></li>
    <% end %>
    </ul>
  <% end %>
<hr>
<p><%= form_for @vendor_sweet do |f| %></p>
<p><%= f.label :price, "Price" %>
   <%= f.text_field :price %></p>
<p><%= f.label :vendor_id, "Vendor" %>
   <%= f.collection_select :vendor_id, @vendors, :id, :name %></p>
<p><%= f.label :sweet_id, "Sweet" %>
   <%= f.collection_select :sweet_id, @sweets, :id, :name %></p>
<hr>
<p><%= f.submit "Create VendorSweet" %></p>
<% end %>
</main>